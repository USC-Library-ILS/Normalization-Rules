rule "Primo VE - Genre 630"
	when
        MARC is "630"."v"
	then
	    set TEMP"1" to MARC."630"."v"
        kormarc remove nonfiling brackets MARC."630" sourcetag "630" from TEMP"1"
        remove substring using regex (TEMP"1","\\.+$")
        create pnx."display"."genre" with TEMP"1"
end

