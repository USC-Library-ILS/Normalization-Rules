rule "Primo VE - Genre 880-600"
	when
        MARC is "880"."v" AND
        MARC."880"."6" match "600-.*"
	then
        set TEMP"1" to MARC."880"."v" 
		remove substring using regex (TEMP"1","\\.+$")
        create pnx."display"."genre" with TEMP"1"
end

