rule "Primo VE - Genre 600"
	when
        MARC is "600"."v"
	then
		set TEMP"1" to MARC."600"."v" 
		remove substring using regex (TEMP"1","\\.+$")
        create pnx."display"."genre" with TEMP"1"
end

rule "Primo VE - Genre 610"
